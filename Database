CREATE SCHEMA `companysales`;
CREATE TABLE `comapny`.`companysales` (
  `ExpenditureID` INT NOT NULL,
  `CompanyName` VARCHAR(45) NOT NULL,
  `AnnualIncome` INT NOT NULL,
  `ExpenseType` VARCHAR(45) NOT NULL,
  `Amount` INT NOT NULL,
  `Year` INT NOT NULL,
  `Department` VARCHAR(45) NOT NULL,
  `PaymentMethod` VARCHAR(45) NOT NULL,
  PRIMARY KEY (`ExpenditureID`));
